@model ViewDataDictionary

<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width: 100%">
        <thead class="thead-dark">
        <tr class="table-info">
            @foreach (var header in Model.Values)
            {
                <th>@header</th>
            }
            <th></th>
        </tr>
        </thead>
    </table>
</div>